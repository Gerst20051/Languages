<?xml version="1.0" ?>
<resource>
  <object class="wxScrollingDialog" name="dlgAddToDo">
    <title>Add Todo item</title>
    <centered>1</centered>
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <object class="wxStaticText">
          <label>Text:</label>
        </object>
        <flag>wxTOP|wxLEFT|wxRIGHT|wxEXPAND</flag>
        <border>8</border>
      </object>
      <object class="sizeritem">
        <object class="wxTextCtrl" name="txtText">
          <style>wxTE_MULTILINE</style>
        </object>
        <option>1</option>
        <flag>wxBOTTOM|wxLEFT|wxRIGHT|wxEXPAND</flag>
        <border>8</border>
      </object>
      <object class="sizeritem">
        <object class="wxFlexGridSizer">
          <cols>2</cols>
          <hgap>8</hgap>
          <object class="sizeritem">
            <object class="wxStaticText">
              <label>User:</label>
            </object>
          </object>
          <object class="spacer">
            <size>0,0</size>
          </object>
          <object class="sizeritem">
            <object class="wxChoice" name="chcUser">
              <content/>
            </object>
            <flag>wxBOTTOM|wxEXPAND</flag>
            <border>8</border>
          </object>
          <growablecols>0</growablecols>
          <object class="sizeritem">
            <object class="wxBoxSizer">
              <orient>wxHORIZONTAL</orient>
              <object class="sizeritem">
                <object class="wxButton" name="btAddUser">
                  <label>Add new user</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxButton" name="btDelUser">
                  <label>Delete user</label>
                </object>
              </object>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxStaticText">
              <label>Type:</label>
            </object>
          </object>
          <object class="spacer"/>

          <object class="sizeritem">
            <object class="wxChoice" name="chcType">
              <content/>
            </object>
            <flag>wxBOTTOM|wxEXPAND</flag>
            <border>8</border>
          </object>
          <object class="sizeritem">
            <object class="wxBoxSizer">
              <orient>wxHORIZONTAL</orient>
              <object class="sizeritem">
                <object class="wxButton" name="btAddType">
                  <label>Add new type</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxButton" name="btDelType">
                  <label>Delete type</label>
                </object>
              </object>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxStaticText">
              <label>Priority (1-9):</label>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxStaticText">
              <label>Position:</label>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxSpinCtrl" name="spnPriority">
              <value>1</value>
              <min>1</min>
              <max>9</max>
            </object>
            <flag>wxBOTTOM|wxEXPAND</flag>
            <border>8</border>
          </object>

          <object class="sizeritem">
            <object class="wxChoice" name="chcPosition">
              <content>
                <item>New line above current line</item>
                <item>Exactly where caret is now</item>
                <item>New line below current line</item>
              </content>
              <selection>0</selection>
            </object>
            <flag>wxBOTTOM|wxEXPAND</flag>
            <border>8</border>
          </object>
          <object class="sizeritem">
            <object class="wxStaticText">
              <label>Comment style:</label>
            </object>
          </object>
          <object class="spacer"/>
          <object class="sizeritem">
            <object class="wxChoice" name="chcStyle">
              <content>
                <item>C++ style comment</item>
                <item>C style comment</item>
                <item>Doxygen style comment</item>
                <item>Compiler warning</item>
                <item>Compiler error</item>
              </content>
              <selection>0</selection>
            </object>
            <flag>wxBOTTOM|wxEXPAND</flag>
            <border>8</border>
          </object>
          <object class="spacer"/>
        </object>
        <option>1</option>
        <flag>wxLEFT|wxRIGHT|wxEXPAND</flag>
        <border>8</border>
      </object>
      <object class="sizeritem">
        <object class="wxStaticLine"/>
        <flag>wxALL|wxEXPAND</flag>
        <border>8</border>
      </object>
      <object class="sizeritem">
        <object class="wxBoxSizer">
          <orient>wxHORIZONTAL</orient>
          <object class="sizeritem">
            <object class="wxButton" name="wxID_OK">
              <label>&amp;OK</label>
              <default>1</default>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxButton" name="wxID_CANCEL">
              <label>&amp;Cancel</label>
            </object>
            <flag>wxLEFT</flag>
            <border>8</border>
          </object>
        </object>
        <flag>wxBOTTOM|wxLEFT|wxRIGHT|wxALIGN_CENTRE_HORIZONTAL</flag>
        <border>8</border>
      </object>
    </object>
    <style>wxDEFAULT_DIALOG_STYLE|wxRESIZE_BORDER</style>
  </object>
</resource>
