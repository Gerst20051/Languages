<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="DevPak" />
		<Option pch_mode="0" />
		<Option default_target="default" />
		<Option compiler="gcc" />
		<Build>
			<Target title="libbz2">
				<Option output="bzip2\libbz2.a" prefix_auto="0" extension_auto="0" />
				<Option working_dir="" />
				<Option object_output="..\..\..\.objs\plugins\contrib\devpak_plugin" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-mthreads" />
				</Compiler>
				<Linker>
					<Add option="-mthreads" />
				</Linker>
			</Target>
			<Target title="default">
				<Option output="..\..\..\devel\share\CodeBlocks\plugins\devpakupdater.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="..\..\..\.objs\plugins\contrib\devpak_plugin" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log -na -ns -nd" />
				<Option host_application="..\..\..\devel\codeblocks.exe" />
				<Option projectResourceIncludeDirsRelation="0" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-Winvalid-pch" />
					<Add option="-mthreads" />
					<Add option="-fexceptions" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DWX_PRECOMP" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DcbDEBUG" />
					<Add option="-DwxUSE_UNICODE" />
					<Add directory="..\..\..\include" />
				</Compiler>
				<Linker>
					<Add option="-mthreads" />
					<Add library="bz2" />
					<Add directory="..\..\..\devel" />
				</Linker>
				<ExtraCommands>
					<Add after="zip ..\..\..\devel\share\CodeBlocks\devpakupdater.zip manifest.xml *.xrc" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_CFG" value="" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="libbz2;default;" />
		</VirtualTargets>
		<Compiler>
			<Add directory="bzip2" />
			<Add directory="$(#WX.include)" />
			<Add directory="$(#WX.lib)\gcc_dll$(WX_CFG)\msw$(WX_SUFFIX)" />
			<Add directory="$(#WX)\contrib\include" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WX.include)" />
		</ResourceCompiler>
		<Linker>
			<Add library="codeblocks" />
			<Add library="wxmsw28$(WX_SUFFIX)" />
			<Add directory="bzip2" />
			<Add directory="$(#WX.lib)\gcc_dll$(WX_CFG)" />
		</Linker>
