<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="EnvVars" />
		<Option pch_mode="0" />
		<Option default_target="default" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="..\..\..\devel\share\codeblocks\plugins\envvars.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="..\..\..\.objs\plugins\contrib\envvars" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log -na -ns -nd" />
				<Option host_application="..\..\..\devel\codeblocks.exe" />
				<Compiler>
					<Add option="-DCB_PRECOMP" />
					<Add option="-DWX_PRECOMP" />
				</Compiler>
			</Target>
			<Target title="no_pch">
				<Option output="..\..\..\devel\share\codeblocks\plugins\envvars.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="..\..\..\.objs\plugins\contrib\envvars" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log -na -ns -nd" />
				<Option host_application=".\codeblocks.exe" />
				<Compiler>
					<Add option="-DNOPCH" />
				</Compiler>
			</Target>
			<Environment>
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_CFG" value="" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-g" />
			<Add option="-Winvalid-pch" />
			<Add option="-mthreads" />
			<Add option="-fexceptions" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DcbDEBUG" />
			<Add option="-DwxUSE_UNICODE" />
			<Add directory="..\..\..\include" />
			<Add directory="..\..\..\sdk\wxscintilla\include" />
			<Add directory="$(#WX.INCLUDE)" />
			<Add directory="$(#WX)\contrib\include" />
			<Add directory="$(#WX.lib)\gcc_dll$(WX_CFG)\msw$(WX_SUFFIX)" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WX.include)" />
		</ResourceCompiler>
		<Linker>
			<Add option="-mthreads" />
			<Add library="wxmsw28$(WX_SUFFIX)" />
			<Add library="codeblocks" />
			<Add directory="..\..\..\devel" />
			<Add directory="$(#WX.lib)\gcc_dll$(WX_CFG)" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ..\..\..\devel\share\codeblocks\envvars.zip manifest.xml *.xrc" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="envvars.cpp" />
		<Unit filename="envvars.h" />
		<Unit filename="envvars.xrc" />
		<Unit filename="envvars_cfgdlg.cpp" />
		<Unit filename="envvars_cfgdlg.h" />
		<Unit filename="envvars_common.cpp" />
		<Unit filename="envvars_common.h" />
		<Unit filename="envvars_prjoptdlg.cpp" />
		<Unit filename="envvars_prjoptdlg.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="project_settings.xrc" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
