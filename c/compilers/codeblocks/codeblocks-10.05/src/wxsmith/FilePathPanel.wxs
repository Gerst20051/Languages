<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
    <object class="wxPanel" name="FilePathPanel">
        <object class="wxBoxSizer" variable="BoxSizer1" member="no">
            <orient>wxVERTICAL</orient>
            <object class="sizeritem">
                <object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="no">
                    <label>Please enter the file&apos;s location and name and&#x0A;whether to add it to the active project.</label>
                </object>
                <flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
                <border>8</border>
            </object>
            <object class="sizeritem">
                <object class="wxStaticText" name="ID_STATICTEXT2" variable="StaticText2" member="no">
                    <label>Filename with full path:</label>
                </object>
                <flag>wxTOP|wxLEFT|wxRIGHT|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
                <border>8</border>
            </object>
            <object class="sizeritem">
                <object class="wxBoxSizer" variable="BoxSizer2" member="yes">
                    <object class="sizeritem">
                        <object class="wxTextCtrl" name="ID_TEXTCTRL1" variable="txtFilename" member="yes">
                            <value>Text</value>
                            <handler function="OntxtFilenameText" entry="EVT_TEXT" />
                        </object>
                        <flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
                        <option>1</option>
                    </object>
                    <object class="sizeritem">
                        <object class="wxButton" name="ID_BUTTON1" variable="btnBrowse" member="yes">
                            <label>...</label>
                            <size>22,22</size>
                            <handler function="OnbtnBrowseClick" entry="EVT_BUTTON" />
                        </object>
                        <flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
                    </object>
                </object>
                <flag>wxBOTTOM|wxLEFT|wxRIGHT|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
                <border>8</border>
            </object>
            <object class="sizeritem">
                <object class="wxStaticText" name="ID_STATICTEXT3" variable="lblGuard" member="yes">
                    <label>Header guard word:</label>
                </object>
                <flag>wxTOP|wxLEFT|wxRIGHT|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
                <border>8</border>
            </object>
            <object class="sizeritem">
                <object class="wxTextCtrl" name="ID_TEXTCTRL2" variable="txtGuard" member="yes">
                    <value>Text</value>
                </object>
                <flag>wxBOTTOM|wxLEFT|wxRIGHT|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
                <border>8</border>
            </object>
            <object class="spacer">
                <flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
            </object>
            <object class="sizeritem">
                <object class="wxCheckBox" name="ID_CHECKBOX1" variable="chkAddToProject" member="yes">
                    <label>Add file to active project</label>
                    <handler function="OnchkAddToProjectChange" entry="EVT_CHECKBOX" />
                </object>
                <flag>wxTOP|wxLEFT|wxRIGHT|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
                <border>8</border>
            </object>
            <object class="sizeritem">
                <object class="wxFlexGridSizer" variable="FlexGridSizer1" member="yes">
                    <cols>2</cols>
                    <growablecols>1</growablecols>
                    <growablerows>1</growablerows>
                    <object class="spacer">
                        <flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
                        <size>16,16</size>
                    </object>
                    <object class="sizeritem">
                        <object class="wxStaticText" name="ID_STATICTEXT4" variable="StaticText4" member="no">
                            <label>In build target(s):</label>
                        </object>
                        <flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
                    </object>
                    <object class="spacer">
                        <flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
                        <size>16,16</size>
                    </object>
                    <object class="sizeritem">
                        <object class="wxCheckListBox" name="ID_CHECKLISTBOX2" variable="clbTargets" member="yes">
                            <style>wxLB_HSCROLL|wxLB_SINGLE</style>
                            <size>222,111</size>
                            <enabled>0</enabled>
                        </object>
                        <flag>wxALL|wxEXPAND|wxFIXED_MINSIZE|wxALIGN_LEFT|wxALIGN_TOP</flag>
                        <option>1</option>
                    </object>
                    <object class="spacer">
                        <flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
                        <size>16,16</size>
                    </object>
                    <object class="sizeritem">
                        <object class="wxBoxSizer" variable="BoxSizer6" member="yes">
                            <object class="sizeritem">
                                <object class="wxButton" name="ID_BUTTON2" variable="btnAll" member="yes">
                                    <label>All</label>
                                    <enabled>0</enabled>
                                    <handler function="OnbtnAllClick" entry="EVT_BUTTON" />
                                </object>
                                <flag>wxRIGHT|wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL</flag>
                                <border>4</border>
                            </object>
                            <object class="sizeritem">
                                <object class="wxButton" name="ID_BUTTON3" variable="btnNone" member="yes">
                                    <label>None</label>
                                    <enabled>0</enabled>
                                    <handler function="OnbtnNoneClick" entry="EVT_BUTTON" />
                                </object>
                                <flag>wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL</flag>
                            </object>
                        </object>
                        <flag>wxALL|wxALIGN_RIGHT|wxALIGN_TOP</flag>
                    </object>
                </object>
                <flag>wxBOTTOM|wxLEFT|wxRIGHT|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
                <border>8</border>
                <option>1</option>
            </object>
            <object class="sizeritem">
                <object class="wxBoxSizer" variable="BoxSizer3" member="yes">
                    <orient>wxVERTICAL</orient>
                </object>
                <flag>wxBOTTOM|wxLEFT|wxRIGHT|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
                <border>8</border>
            </object>
        </object>
    </object>
</wxsmith>
